{
    "version": 2,
    "builds": [
        {
            "src": "build/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/build/api/index.js"
        },
        {
            "source": "/(.+)",
            "destination": "/"
        }
    ]
}