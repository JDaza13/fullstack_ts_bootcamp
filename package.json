    "@tanstack/react-query-devtools": "^5.31.0",
    "@types/express": "^4.17.21",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^11.2.6",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "mongodb": "^6.6.0",
    "mongoose": "^8.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
